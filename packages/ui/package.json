{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup ./src/index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./src/styles/styles.css -o ./dist/styles.css",
    "dev": "tsup ./src/index.tsx --format esm,cjs --dts --external react --watch && tailwindcss -i ./src/styles/styles.css -o ./dist/styles.css --watch",
    "clean": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules/",
    "lint": "TIMING=1 eslint \"**/*.ts*\""
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "eslint-config-next": "^13.1.1",
    "firebase": "^9.15.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next": "^13.1.1",
    "next-google-ads": "^0.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.27",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint-config-custom": "*",
    "prettier": "2.7.1",
    "rollup": "^3.10.0",
    "tailwindcss": "^3.2.4",
    "tsconfig": "*",
    "tsup": "^6.5.0",
    "typescript": "4.7.4",
    "web-vitals": "^3.0.1"
  }
}
