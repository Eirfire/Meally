{
  "name": "meally",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "firebase:start": "firebase emulators:start  --import=./seed",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "node ./src/scripts/generate-sitemap.mjs",
    "start": "next start",
    "lint": "next lint",
    "clean:build": "rm -rf .next",
    "clean:node": "rm -rf node_modules/",
    "clean:turbo": "rm -rf .turbo",
    "clean:node_modules": "rm -rf node_modules/",
    "clean:all": "pnpm clean:build && pnpm clean:node && pnpm clean:turbo",
    "postinstall": "prisma generate --schema=./src/prisma/schema.prisma",
    "migration:generate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "migration:push": "prisma db push --schema=./src/prisma/schema.prisma",
    "migration:drop": "prisma migrate reset --schema=./src/prisma/schema.prisma"
  },
