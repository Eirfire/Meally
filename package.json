{
  "name": "mixie",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "start": "turbo start",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "clean:all": "turbo run clean:all"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.21.4",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "turbo": "^2.0.12",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.22",
  "workspaces": [
    "packages/*",
    "apps/*",
    "packages/email/*"
  ],
  "trustedDependencies": [
    "@swc/core",
    "@vercel/speed-insights",
    "core-js-pure",
    "esbuild",
    "protobufjs",
    "sharp",
    "supabase"
  ]
}
