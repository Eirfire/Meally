{
  "name": "mixie",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "start": "turbo start",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "supabase-start": "cd packages/supabase && bun db:restart",
    "clean:all": "turbo run clean:all",
    "update:all": "bun update --latest && cd apps/web && bun update --latest && cd ../../packages/email && bun update --latest && cd ../eslint-config && bun update --latest && cd ../tsconfig && bun update --latest && cd ../supabase && bun update --latest && cd ../tailwind-config && bun update --latest && cd ../../"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.21.4",
    "@types/heic-convert": "^2.1.0",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "turbo": "^2.0.14",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.22",
  "workspaces": [
    "packages/*",
    "apps/*",
    "packages/email/*"
  ],
  "trustedDependencies": [
    "@swc/core",
    "@vercel/speed-insights",
    "core-js-pure",
    "esbuild",
    "protobufjs",
    "sharp",
    "supabase"
  ]
}
