{
  "name": "mixie",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "start": "turbo start",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean:all": "turbo run clean:all"
  },
  "devDependencies": {
    "@manypkg/cli": "^0.21.4",
    "eslint": "^9.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "^2.0.9",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.20",
  "workspaces": [
    "packages/*",
    "apps/*",
    "packages/email/*"
  ],
  "trustedDependencies": [
    "@vercel/speed-insights",
    "core-js-pure"
  ]
