{
  "name": "meally",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build",
    "start": "turbo run start",
    "dev": "dotenv -- turbo run dev",
    "dev-local": "turbo run dev-local",
    "lint": "turbo run lint",
    "clean:build": "turbo run clean:build",
    "clean:node": "turbo run clean:node",
    "clean:turbo": "turbo run clean:turbo",
    "clean:all": "turbo run clean:all && rm -rf node_modules/",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "manypkg check"
  },
  "engines": {
    "node": ">=16.12.2"
  },
  "packageManager": "yarn@1.22.19",
  "dependencies": {
    "@manypkg/cli": "^0.20.0",
    "dotenv-cli": "latest",
    "eslint": "^8.34.0",
    "eslint-config-custom": "*",
    "prettier": "2.7.1",
    "turbo": "1.8.3"
  }
}
